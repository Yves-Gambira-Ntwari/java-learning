package gambira_24rp05514.itlevel6y2_b.nestedloop;
import java.util.Scanner;

public class StarPatterns {

	public StarPatterns() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		System.out.println("1. Solid rectangel ");
		for(int i = 0; i<=num; i++) {
			for(int j = num; j>=0; j--) {				
				System.out.print("*");
			}
			System.out.println(" ");
		}
		System.out.println();
		
		System.out.println("4. Inverted left aligned ");
		for(int i = 0; i<=num; i++) {
			for(int j = 0; j<=num; j++) {				
				System.out.print("*");
			}
			System.out.println(" ");
		}
		
		System.out.println();
		
		System.out.println("3. Inverted left aligned ");
		for(int i = 0; i<=num; i++) {
			for(int j = i; j<=num; j++) {				
				System.out.print("*");
			}
			System.out.println(" ");
		}

	}

}
